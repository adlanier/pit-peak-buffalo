datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum PostType {
  PEAK
  PIT
  BUFFALO
}

model Post {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())

  text       String
  type       PostType

  lat       Float
  lng       Float
}

